public class Solution {  

    public static void sortZeroesAndOne(int[] arr) {
         int nonz=0;
       for(int i=0;i<arr.length;i++){
           if(arr[i]==0){
              int temp=arr[nonz];
               arr[nonz]=arr[i];
               arr[i]=temp;
               nonz++;
           }
       }
    
    }
}
