
public class Solution {
	
	public static boolean checkMember(int n){
        int a=-1;
        int b=1;
        int c=0;
        int i=0;
      
        for(i=0;i<=n+1;i++)
        {
            c=a+b;
            a=b;
            b=c;
            if(c==n){
                return true;
            }
        }
        return false;
        
    } 
        
	

}
