import java.util.Scanner;
public class Solution {

	public static void rowWiseSum(int[][] mat) {
		int sum=0;
        for(int i=0;i<mat.length;i++)
        {
            for(int j=0;j<mat[0].length;j++)
            {
                sum=mat[i][j]+sum;
            }
            
            System.out.print(sum+" ");
            sum=0;
        }
	}

}
